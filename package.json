{
  "name": "padoc",
  "version": "0.3.0",
  "description": "ECMAScript compiler for pado.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/sepalang/padoc"
  },
  "main": "index.js",
  "bin": {
    "padoc": "bin/padoc.js"
  },
  "scripts": {
    "clean:sample": "rimraf sample/dist/*",
    "clean": "npm run clean:sample",
    "dist": "node sample/dist",
    "test:compile": "node ./bin/padoc.js sample/src sample/dist",
    "test:compile:umd": "node ./bin/padoc.js sample/src sample/dist -m umd",
    "test:compile:umd:file": "node ./bin/padoc.js sample/src/index.js sample/dist/all.js -m umd",
    "test:compile:pack:umd": "node ./bin/padoc.js --pack sample/src/index.js sample/dist/all.js -m umd",
    "test:compile:pack:amd": "node ./bin/padoc.js --pack sample/src/index.js sample/dist/all.js -m amd",
    "test:compile:pack:iife": "node ./bin/padoc.js --pack sample/src/index.js sample/dist/all.js -m iife",
    "test": "npm run test:compile",
    "lint": "eslint bin",
    "lint:fix": "eslint bin --fix"
  },
  "author": "AHN HO JUNG",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sepalang/padoc/issues"
  },
  "homepage": "https://github.com/sepalang/padoc#readme",
  "dependencies": {
    "@babel/cli": "^7.0.0-beta.42",
    "@babel/core": "^7.0.0-beta.42",
    "@babel/plugin-external-helpers": "^7.0.0-beta.42",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.42",
    "@babel/plugin-transform-modules-amd": "^7.0.0-beta.42",
    "@babel/plugin-transform-modules-commonjs": "^7.0.0-beta.42",
    "@babel/plugin-transform-modules-umd": "^7.0.0-beta.42",
    "@babel/polyfill": "^7.0.0-beta.42",
    "@babel/preset-env": "^7.0.0-beta.42",
    "glob": "^7.1.2",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "package.root": "^1.0.3",
    "rimraf": "^2.6.2",
    "rollup": "^0.57.1",
    "rollup-plugin-babel": "^4.0.0-beta.2",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-node-resolve": "^3.3.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "eslint": "^4.19.0"
  }
}
