{
  "name": "@sepalang/padoc",
  "version": "0.11.0",
  "description": "ASAP compile the ECMAScript",
  "repository": {
    "type": "git",
    "url": "https://github.com/sepalang/padoc"
  },
  "main": "index.js",
  "bin": {
    "padoc": "bin/padoc.js"
  },
  "scripts": {
    "clean:sample": "rimraf sample/dist/*",
    "clean": "npm run clean:sample",
    "test": "npm run test:compile",
    "test:compile": "my padoc test/run-script/src test/run-script/dist",
    "test:pack": "my padoc --pack test/run-script/src/index.js test/run-script/dist/pack.js",
    "test:pack:es": "my padoc --pack test/run-script/src/reborn.js test/run-script/dist/reborn.js -m es",
    "test:pack:m": "my padoc --pack 'test/run-script/src/reborn.js' test/run-script/dist/reborn.js -m es",
    "test:packs": "my padoc --pack 'test/chunk/src/main-a.js test/chunk/src/main-b.js' test/chunk/dist -m es",
    "test:packs:glob": "my padoc --pack 'test/chunk/src/main-*.js' test/chunk/dist -m es",
    "test:exec": "my padoc --exec test/run-script/src/read.js",
    "test:exec:i": "my padoc --exec test/run-script/src/read.js -i",
    "test:exec:ig": "my padoc --exec test/run-script/src/read.js -i -g",
    "test:ignore": "my padoc --test test/ignore --verbose",
    "test:ignore:compile": "my padoc test/ignore/src test/ignore/dist",
    "test:ignore:pack": "my padoc --pack 'test/ignore/src/*.js' test/ignore/dist -m es",
    "test:feature": "my padoc test/feature/src test/feature/dist"
  },
  "author": "AHN HO JUNG",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sepalang/padoc/issues"
  },
  "homepage": "https://github.com/sepalang/padoc#readme",
  "dependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/plugin-external-helpers": "^7.0.0",
    "@babel/plugin-proposal-async-generator-functions": "^7.1.0",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.2.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-proposal-partial-application": "^7.4.4",
    "@babel/plugin-syntax-async-generators": "^7.0.0",
    "@babel/plugin-transform-modules-amd": "^7.1.0",
    "@babel/plugin-transform-modules-commonjs": "^7.1.0",
    "@babel/plugin-transform-modules-umd": "^7.1.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@sepalang/myself": "^0.3.0",
    "@sepalang/runner": "^0.9.0",
    "core-js": "^3.5.0",
    "glob": "^7.1.3",
    "install": "^0.13.0",
    "mkdirp": "^0.5.1",
    "npm": "^6.10.0",
    "rimraf": "^2.6.2",
    "rollup": "^1.2.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-node-resolve": "^5.0.0"
  }
}
