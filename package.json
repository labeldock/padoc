{
  "name": "padoc",
  "version": "0.1.2",
  "description": "ECMAScript compiler for pado.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/sepalang/padoc"
  },
  "main": "index.js",
  "bin": {
    "padoc": "bin/padoc.js"
  },
  "scripts": {
    "clean:sample": "rimraf sample/dist/*",
    "clean": "npm run clean:sample",
    "test:compile": "node ./bin/padoc.js sample/src sample/dist",
    "test:compile:umd": "node ./bin/padoc.js sample/src sample/dist -m umd",
    "test:compile:umd:file": "node ./bin/padoc.js sample/src/index.js sample/dist/all.js -m umd",
    "test:compile:pack:amd": "node ./bin/padoc.js --pack sample/src/index.js sample/dist/all.js -m umd",
    "test:compile:pack:iife": "node ./bin/padoc.js --pack sample/src/index.js sample/dist/all.js -m umd",
    "test": "npm run test:compile",
    "lint": "eslint bin",
    "lint:fix": "eslint bin --fix"
  },
  "author": "AHN HO JUNG",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sepalang/padoc/issues"
  },
  "homepage": "https://github.com/sepalang/padoc#readme",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "glob": "^7.1.2",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "package.root": "^1.0.3",
    "rimraf": "^2.6.2",
    "rollup": "^0.56.3",
    "rollup-plugin-commonjs": "^8.3.0",
    "rollup-plugin-node-resolve": "^3.0.3"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "eslint": "^4.18.2"
  }
}
